package com.VariableExample;

public class VariableExamples {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		short shortvariable=21;
		System.out.println("the value of variable shortvariable is:-" +shortvariable);
		short Nikky=07;
		System.out.println("the value of variable is:-" +Nikky);
		
		System.out.println(Nikky);
		System.out.println(shortvariable);
		
		short chinttu=100;
		System.out.println("the value of variable is:-"+chinttu);
		System.out.println(chinttu);
		System.out.println(Nikky);
		System.out.println(shortvariable);
		System.out.println("the value of variable is:-"+shortvariable);
		short shortgirl=5;
		short longboy=6;
		System.out.println("the value of variable is:-"+longboy);
		System.out.println(longboy);
		System.out.println(shortgirl);
		System.out.println("the value of variable is:-"+shortgirl);
		System.out.println();
		
		int intvarible=65;
		System.out.println("the value of the varible intvariable is:-"+intvarible);

	}

}
