		byte Madhu[][]=new byte[3][3];                                                                                        
		
        Madhu[0][0]='2';                                                                       
		Madhu[0][1]='2';
		Madhu[0][2]='2';
		
		Madhu[1][0]='3';
		Madhu[1][1]='3';
		Madhu[1][2]='3';
		
		Madhu[2][0]='1';
		Madhu[2][1]='1';
		Madhu[2][2]='1';
		
		
		for(byte rowIndex=0; rowIndex<=3; rowIndex++) 
		{
			for(byte rowOfCell=0; rowOfCell<=2; rowOfCell++)
			{
				System.out.println(Madhu[rowOfCell][rowOfCell] + "   "); 
				System.out.println();
				System.out.println(Madhu[rowIndex][rowIndex] + "   ");
			//	System.out.println(Madhu[rowOfCell][rowOfCell] + "   ");
			}
